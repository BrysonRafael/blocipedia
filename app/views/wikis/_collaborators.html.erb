<% if current_user != nil && current_user.is_premium && @wiki.private %>
  <div class="form-group">
    <%= f.label :collaborators, class: 'checkbox' do %>
      <%= f.text_field :collaborators, class: 'form-control', placeholder: "Collaborators" %>
      <%= form_tag({:collaborators => 'create'}, {:id => @user.id}) %>
    <% end %>
  </div>
<% end %>